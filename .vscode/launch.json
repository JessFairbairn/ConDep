{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Parse Text- fixed input", "type": "python", "request": "launch", 
            "stopOnEntry": false, 
            "pythonPath": "${config:python.pythonPath}", 
            "program": "${workspaceFolder}/parse_text.py",
            "cwd": "${workspaceFolder}",
            "console":"integratedTerminal",
            "env": {}, 
            "args": [ "star accretes a particle" ], 
            "envFile": "${workspaceFolder}/.env", 
            "debugOptions": [], 
            "internalConsoleOptions": "neverOpen" 
        },
        {
            "name": "Parse Text with prompt", "type": "python", "request": "launch", 
            "stopOnEntry": false, 
            "pythonPath": "${config:python.pythonPath}", 
            "program": "${workspaceFolder}/parse_text.py",
            "cwd": "${workspaceFolder}",
            "console":"integratedTerminal",
            "env": {}, 
            "args": [ ], 
            "envFile": "${workspaceFolder}/.env", 
            "debugOptions": [], 
            "internalConsoleOptions": "neverOpen" 
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}"
        },
        {
            "name":"Prolog",
            "type": "prolog",
            "request": "launch",
            "program": "${file}",
            "startupQuery": "start",
            "cwd": "${workspaceRoot}/prolog",
            "runtimeExecutable": "/usr/bin/prolog",
            "runtimeArgs": [],
            "traceCmds": {
                "continue": [
                    "leap",
                    "l"
                ],
                "stepover": [
                    "skip",
                    "s"
                ],
                "stepinto": [
                    "creep",
                    "c"
                ],
                "stepout": [
                    "up",
                    "u"
                ]
            }
        }
    ]
}